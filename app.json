{
  "expo": {
    "name": "Anime Nation",
    "slug": "animenation",
    "privacy": "public",
    "description": "Anime Nation - Application that offers vast knowledge about some prestigious and award winning animated movies from Studio Ghibli",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/signInUp/titleImage.png",
    "splash": {
      "image": "./assets/signInUp/coverPhoto.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/signInUp/titleImage.png"
    }
  }
}